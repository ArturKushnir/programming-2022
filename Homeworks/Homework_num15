// знайти місяць за номером
#include <iostream>
#include <string>
#include <math.h>

using namespace std;

int main()
{
    int a = 0;
    cout << "enter the number month" << a << endl;
    cin >> a;

    if (a == 1)
    {
        cout << "January" << endl;
    }

    else if (a == 2)
    {
        cout << "February" << endl;
    }

    else if (a == 3)
    {
        cout << "March" << endl;
    }

    else if (a == 4)
    {
        cout << "April" << endl;
    }

    else if (a == 5)
    {
        cout << "May" << endl;
    }

    else if (a == 6)
    {
        cout << "June" << endl;
    }

    else if (a == 7)
    {
        cout << "July" << endl;
    }

    else if (a == 8)
    {
        cout << "August" << endl;
    }

    else if (a == 9)
    {
        cout << "September" << endl;
    }

    else if (a == 10)
    {
        cout << "October" << endl;
    }

    else if (a == 11)
    {
        cout << "November" << endl;
    }

    else if (a == 12)
    {
        cout << "December" << endl;
    }

    else if (a < 1)
    {
        cout << "such a month does not exist" << endl;
    }

    else if (a > 12)
    {
        cout << "such a month does not exist" << endl;
    }

    return 0;
}
