#include <iostream>
#include <math.h>

using namespace std;

int main()
{
	int day = 0;
	cout << "Enter the day" << day;
	cin >> day;

	int month = 0;
	cout << "Enter the month" << month;
	cin >> month;

	if (day >= 21 <= 31 && month == 3)
	{
		cout << "Aries" << endl;
	}  
	else if (day >= 1 <= 20 && month == 4 )
	{
		cout << "Aries" << endl;
	}

	else if (day >= 21 <= 30 && month == 4)
	{
		cout << "Taurus" << endl;
	}
	else if (day >= 1 <= 21 && month == 5)
	{
		cout << "Taurus" << endl;
	}

	else if (day >= 22 <= 31 && month == 5)
	{
		cout << "twins" << endl;
	}
	else if (day >= 1 <= 21 && month == 6)
	{
		cout << "twins" << endl;
	}

	else if (day >= 22 <= 30 && month == 6)
	{
		cout << "crawfish" << endl;
	}
	else if (day >= 1 <= 22 && month == 7)
	{
		cout << "crawfish" << endl;
	}

	else if (day >= 23 <= 31 && month == 7)
	{
		cout << "Lion" << endl;
	}
	else if (day >= 1 <= 21 && month == 8)
	{
		cout << "Lion" << endl;
	}

	else if (day >= 22 <= 31 && month == 8)
	{
		cout << "Virgo" << endl;
	}
	else if (day >= 1 <= 23 && month == 9)
	{
		cout << "Virgo" << endl;
	}

	else if (day >= 24 <= 30 && month == 9)
	{
		cout << "Libra" << endl;
	}
	else if (day >= 1 <= 23 && month == 10)
	{
		cout << "Libra" << endl;
	}

	else if (day >= 24 <= 31 && month == 10)
	{
		cout << "Scorpio" << endl;
	}
	else if (day >= 1 <= 22 && month == 11)
	{
		cout << "Scorpio" << endl;
	}

	else if (day >= 23 <= 30 && month == 11)
	{
		cout << "Sagittarius" << endl;
	}
	else if (day >= 1 <= 22 && month == 12)
	{
		cout << "Sagittarius" << endl;
	}

	else if (day >= 23 <= 31 && month == 12)
	{
		cout << "Ibex" << endl;
	}
	else if (day >= 1 <= 20 && month == 1)
	{
		cout << "Ibex" << endl;
	}

	else if (day >= 21 <= 31 && month == 1)
	{
		cout << "Aquarius" << endl;
	}
	else if (day >= 1 <= 19 && month == 2)
	{
		cout << "Aquarius" << endl;
	}

	else if (day >= 20 <= 29 && month == 2)
	{
		cout << "Fish" << endl;
	}
	else if (day >= 1 <= 20 && month == 3)
	{
		cout << "Fish" << endl;
	}

}
